.stepper-btn
  a.btn.btn--default(href='assets/files/card14.pdf', target='_blank')
    span Скачать опросный лист
      br
      small .PDF&nbsp;&nbsp;235 KB.
    i.icon-download
form.steps-form-valid.stepper-two(action='#')
  div
    h3 1
    section
      h4 Материал сепарации
      .row.middle__xs
        .col.col__xs-12.form__group: +fieldLabelInputId('id-1', 'Наименование*', true)
      label.form__group Фракционный состав, мм
      .row.middle__xs
        .col.col__lg-3.col__md-6.col__xs-12.form__group: +fieldLabelInputId('id-3', 'от*', true, 'isNumber')
        .col.col__lg-3.col__md-6.col__xs-12.form__group: +fieldLabelInputId('id-4', 'до*', true, 'isNumber')
        .col.col__md-6.col__xs-12.form__group: +fieldLabelInputId('id-5', 'Влажность материала, %*', true, 'isNumber')
        .col.col__md-6.col__xs-12.form__group: +fieldLabelInputId('id-6', 'Производительность, т/ч*', true, 'isNumber')
      h4 Состав материала сепарации
      h5 Электропроводящий продукт
      .row.middle__xs
        .col.col__xs-12.form__group: +fieldLabelInputId('id-7', 'Наименование', false)
        .col.col__lg-4.col__md-6.col__xs-12.form__group: label Фракционный состав, мм
        .col.col__lg-3.col__md-6.col__xs-12.form__group: +fieldLabelInputId('from1', 'от', false, 'isNumber')
        .col.col__lg-3.col__md-6.col__xs-12.form__group: +fieldLabelInputId('to1', 'до', false, 'isNumber')
      h5 Полупроводниковый продукт
      .row.middle__xs
        .col.col__xs-12.form__group: +fieldLabelInputId('id-8', 'Наименование', false)
        .col.col__lg-4.col__md-6.col__xs-12.form__group: label Фракционный состав, мм
        .col.col__lg-3.col__md-6.col__xs-12.form__group: +fieldLabelInputId('from2', 'от', false, 'isNumber')
        .col.col__lg-3.col__md-6.col__xs-12.form__group: +fieldLabelInputId('to2', 'до', false, 'isNumber')
      h5 Не проводящий продукт
      .row.middle__xs
        .col.col__xs-12.form__group: +fieldLabelInputId('id-9', 'Наименование', false)
        .col.col__lg-4.col__md-6.col__xs-12.form__group: label Фракционный состав, мм
        .col.col__lg-3.col__md-6.col__xs-12.form__group: +fieldLabelInputId('from3', 'от', false, 'isNumber')
        .col.col__lg-3.col__md-6.col__xs-12.form__group: +fieldLabelInputId('to3', 'до', false, 'isNumber')
      br
      .stepper-next: +btnArrowSub('Шаг 2', 'btn--full')
    h3 2
    include ../components/_stepperLast1
