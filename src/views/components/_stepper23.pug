        .col.col__xs-12.form__group: +fieldLabelInputId('id-5', 'Рабочая среда*', true)
        .col.col__xs-12.form__group: +fieldLabelInputId('id-6', 'Давление рабочей среды внутри разделительного экрана, Атм*', true, 'isNumber')
