.stepper-btn
  a.btn.btn--default.popup-modal.popup-with-zoom-anim(href='#call')
    span Скачать опросный лист
      br
      small .PDF&nbsp;&nbsp;235 KB.
    i.icon-download
form.steps-form.stepper-two(action='#')
  div
    h3 1
    section
      h4 Наименование и описание показателей
      h5 Транспортируемый груз
      .row
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pol', 'pol', 'Тип груза', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pol1', 'pol1', 'Насыпная плотность, Y, м/м³', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pol2', 'pol2', 'Влажность материала, %', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pol3', 'pol3', 'Максимальный размер кусков, мм', false)
      h5 Металлические (ферромагнитные включения)
      .row
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pol4', 'pol4', 'Тип, происхождение включений', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pol5', 'pol5', 'Фракционный состав, мм', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pol6', 'pol6', 'Фактическое содержание примесей, %', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pol7', 'pol7', 'Допустимое содержание примесей, %', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pol8', 'pol8', 'Часовой грузопоток, Q т/час или м³/час', false)
      h5 Производительность
      .form__group
        +fieldLabelInput('text', 'pol9', 'pol9', 'Масса включения, кг', false)
      h5 Конвейерная лента
      .row
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok', 'pok', 'Ширина ленты, В, мм', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok1', 'pok1', 'Ширина материала на ленте, b, мм', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok2', 'pok2', 'Высота материалал на ленте, h, мм', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok3', 'pok3', 'Скорость движения ленты, v, м/с', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok4', 'pok4', 'Толщина ленты', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok5', 'pok5', 'Температура окражающей среды, °С', false)
      h5 Используемый тип роликоопор
      .form__group
        +fieldLabelInput('text', 'pok6', 'pok6', 'Верхняя ветвь: желобчатая, плоская', false)
      h5 Приводной барабан
      .row
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok7', 'pok7', 'Наличие футеровки', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok8', 'pok8', 'Диаметр (с футеровкой), мм', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok9', 'pok9', 'Общая установленная мощность электродвигателя, кВт', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok10', 'pok10', 'Частота вращения вала электродвигателя, об/мин', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok11', 'pok11', 'Общая установленная мощность электродвигателя, кВт', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok12', 'pok12', 'Частота вращения вала электродвигателя, об/мин', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok13', 'pok13', 'Количество консолей вала, шт', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok14', 'pok14', 'Количество шпонок на консоли, шт', false)
        .col.col__md-6.col__xs-12.form__group
          +fieldLabelInput('text', 'pok15', 'pok15', 'Крутящий момент на барабане, н/м', false)
      br
      .stepper-next: +btnArrowSub('Шаг 2', 'btn--full')
    h3 2
    include ../components/_stepperLast
