extends ../layouts/master.pug

block pageName
  - var pageName = "fxindex"

block header
  include ../partials/_headerFX.pug

block content
  .container
    h1
      +reveal(['Глоссарий'])
    +breadcrumbs(['Глоссарий'])
    .glossary
      .row
        .col.col__lg-3.col__md-4.col__xs-12
          ul.aside-menu#filters
            li: a.active(href='#', data-filter="all"): span Все
            li: a(href='#', data-filter=".earth"): span Оборудование
            li: a(href='#', data-filter=".air"): span Приборы
            li: a(href='#', data-filter=".fire"): span Материалы
            li: a(href='#', data-filter=".water"): span Химические элементы
        .col.col__lg-9.col__md-8.col__xs-12
          .accordion.grid
            .accordion-item.earth(data-aos="fade-up")
              .accordion-header
                .title Ални, Альни
                .helper Материал
                .plus
                  i.icon-plus
              .accordion-content (англ. Alni) - магнитотвердый сплав алюминия, никеля и железа, получаемый методами литья или порошковой металлургии. Свойства и обозначение зависят от соотношения элементов и способа получения. Применяется для изготовления постоянных магнитов. См. также ЮНД, ЮНДК.
            .accordion-item.air(data-aos="fade-up")
